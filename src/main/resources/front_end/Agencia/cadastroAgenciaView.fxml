<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #fcfdff;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="front_end.Agencia.cadastroAgenciaController">
   <children>
      <Label alignment="CENTER" prefHeight="45.0" prefWidth="600.0" style="-fx-background-color: #4399A9;" text="Cadastro de Agencia" textFill="#fcfdff">
         <font>
            <Font name="Arial Black" size="27.0" />
         </font>
      </Label>
      <TextFlow layoutX="14.0" layoutY="72.0">
         <children>
            <Label prefHeight="17.0" prefWidth="69.0" text="Numero:" />
            <TextField id="nome_field" fx:id="number_field" />
         </children>
      </TextFlow>
      <TextFlow layoutX="14.0" layoutY="109.0">
         <children>
            <Label prefHeight="17.0" prefWidth="69.0" text="Cidade:" />
            <TextField fx:id="city_field" />
         </children>
      </TextFlow>
      <TextFlow layoutX="14.0" layoutY="149.0">
         <children>
            <Label prefHeight="17.0" prefWidth="69.0" text="Estado" />
            <TextField fx:id="est_field" />
         </children>
      </TextFlow>
      <AnchorPane layoutX="14.0" layoutY="188.0">
         <children>
            <Label layoutY="4.0" prefHeight="17.0" prefWidth="69.0" text="Conta:" />
            <ChoiceBox layoutX="69.0" prefWidth="150.0" style="-fx-background-color: #4399A9;" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="14.0" layoutY="255.0">
         <children>
            <Button mnemonicParsing="false" onAction="#Submit" style="-fx-background-color: #4399A9;" text="Enviar" textFill="#fcfdff" />
            <Label fx:id="teste" layoutX="137.0" layoutY="4.0" prefHeight="17.0" prefWidth="38.0" textFill="#00ff4d">
               <font>
                  <Font name="Arial Black" size="13.0" />
               </font>
            </Label>
            <Button layoutX="69.0" mnemonicParsing="false" onAction="#returnMenu" text="Voltar" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
